{{#each blogs}}
<div class="blog-container">
    <div class="blog-title">
        <a href="/api/blogs/{{id}}">{{title}}</a>
        <h6>Posted on {{format_date createdAt}}</h6>
    </div>
    <div class="blog-text">
        <h5>{{text}}</h5>
    </div>
</div>
{{/each}}

<button type="submit" class="btn"><a class="link" href="/dashboard/new">+ New Post</a></button>
