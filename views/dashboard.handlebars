{{#if blogs.length}}
{{#each blogs}}
<div class="blog-container">
    <div class="blog-title">
        <h5>{{title}}</h5>
        <h6>{{format_date createdAt}}</h6>
    </div>
    <div class="blog-text">
        <h5>{{text}}</h5>
    </div>
</div>
{{/each}}
{{/if}}

<button type="submit" class="btn"><a class="link" href="/dashboard/new">+ New Blog</a></button>

<script src="/js/addBlog.js"></script>